[database]
sqlalchemy.url = sqlite:///
sqlalchemy.echo = false

[loggers]
keys = aybu

[handlers]
keys = console

[formatters]
keys = generic

[logger_aybu]
level = WARN
handlers =
qualname = aybu

[handler_console]
class = StreamHandler
args = (sys.stderr,)
level = NOTSET
formatter = generic

[formatter_generic]
format = %(asctime)s %(levelname)-5.5s [%(name)s][%(threadName)s] %(message)s

